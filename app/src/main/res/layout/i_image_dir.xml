<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:clipToPadding="true"
    android:fitsSystemWindows="true"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    >
    <include
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        layout="@layout/ndian_title"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:id="@+id/include" />
    <ListView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:clipToPadding="true"
        android:fitsSystemWindows="true"
        android:id="@+id/listview"
        android:paddingTop="@dimen/activity_vertical_margin"
        android:paddingBottom="@dimen/activity_vertical_margin"
        android:layout_below="@+id/include"
        android:layout_above="@+id/relativeLayout7"></ListView>

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/imageView27"
        android:background="@drawable/xian_gray"
        android:layout_above="@+id/relativeLayout7"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/list_height"
        android:background="@color/white"
        android:id="@+id/relativeLayout7"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="@dimen/list_height"
            android:text="@string/cancel"
            android:textColor="@color/press_gray_gray_photo"
            android:gravity="center"
            android:textSize="@dimen/fifteen_thirteen"
            android:paddingLeft="@dimen/fifteen"
            android:paddingRight="@dimen/fifteen"
            android:id="@+id/cancel"
            android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />
    <RelativeLayout
        android:layout_width="@dimen/eighty"
        android:layout_height="@dimen/list_height"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:id="@+id/achieveRelativeLayout"
        android:layout_alignParentEnd="true">
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="@dimen/list_height"
        android:text="@string/achieve"
        android:textColor="@color/press_gray_gray_photo"
        android:gravity="center"
        android:textSize="@dimen/fifteen_thirteen"
        android:paddingLeft="@dimen/fifteen"
        android:paddingRight="@dimen/fifteen"
        android:id="@+id/achieve"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

        <TextView
            android:layout_width="15dip"
            android:layout_height="15dip"
            android:text="9"
            android:gravity="center"
            android:layout_marginRight="@dimen/forty_two"
            android:layout_marginTop="@dimen/six"
            android:textColor="@color/white"
            android:textSize="@dimen/eleven_nine"
            android:background="@drawable/press_kuang_corners_dian_ten"
            android:id="@+id/number"
            android:layout_alignParentTop="true"
            android:layout_alignRight="@+id/achieve"
            android:layout_alignEnd="@+id/achieve" />
    </RelativeLayout>
    </RelativeLayout>

</RelativeLayout>

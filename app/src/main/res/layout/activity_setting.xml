<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/background"
    android:clipToPadding="true"
    android:fitsSystemWindows="true"
    tools:context=".activity.me.view.SettingActivity">
    <include
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        layout="@layout/ndian_title"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:id="@+id/include" />
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:layout_below="@+id/include">
        <RelativeLayout
            android:id="@+id/MessageNotificationRelativeLayout"
            android:layout_width="match_parent"
            android:layout_height="@dimen/list_height"
            android:background="@drawable/press_white_gray_relative" >

            <TextView
                android:id="@+id/textView1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/fifteen"
                android:textColor="@color/text_black"
                android:textSize="@dimen/fifteen_thirteen"
                android:text="@string/message_notification"
                 />

            <ImageView
                android:id="@+id/imageView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="@dimen/fifteen"
                android:src="@drawable/index_fenlei3x" />


        </RelativeLayout>


        <RelativeLayout
            android:id="@+id/scoreRelativeLayout"
            android:layout_width="match_parent"
            android:layout_height="@dimen/list_height"
            android:layout_marginTop="@dimen/ten"
            android:background="@drawable/press_white_gray_relative" >

            <ImageView
                android:id="@+id/imageView9"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="@dimen/fifteen"
                android:src="@drawable/index_fenlei3x" />

            <TextView
                android:id="@+id/textView17"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/fifteen"
                android:textColor="@color/text_black"
                android:textSize="@dimen/fifteen_thirteen"
                android:text="@string/To_score" />

            <ImageView
                android:id="@+id/imageView10"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:background="@drawable/xian_gray"
               />
        </RelativeLayout>


        <RelativeLayout
            android:id="@+id/CacheRelativeLayout"
            android:layout_width="match_parent"
            android:layout_height="@dimen/list_height"
            android:background="@drawable/press_white_gray_relative" >


            <TextView
                android:id="@+id/textView38"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/fifteen"
                android:textColor="@color/text_black"
                android:textSize="@dimen/fifteen_thirteen"
                android:text="@string/Clear_the_cache" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="0.00MB"
                android:layout_marginRight="@dimen/fifteen"
                android:textColor="@color/text_gray"
                android:textSize="@dimen/twelve_ten"
                android:id="@+id/CacheTextView"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true" />
            <ImageView
                android:id="@+id/imageView12"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:background="@drawable/xian_gray"
                />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/aboutRelativeLayout"
            android:layout_width="match_parent"
            android:layout_height="@dimen/list_height"
            android:background="@drawable/press_white_gray_relative" >

            <ImageView
                android:id="@+id/imageView13"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="@dimen/fifteen"
                android:src="@drawable/index_fenlei3x" />

            <TextView
                android:id="@+id/textView19"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/fifteen"
                android:textColor="@color/text_black"
                android:textSize="@dimen/fifteen_thirteen"
                android:text="@string/about" />

        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/logoutRelativeLayout"
            android:layout_width="match_parent"
            android:layout_marginTop="@dimen/ten"
            android:layout_height="@dimen/list_height"
            android:background="@drawable/press_white_gray_relative" >

            <ImageView
                android:id="@+id/imageView11"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="@dimen/fifteen"
                android:src="@drawable/index_fenlei3x" />

            <TextView
                android:id="@+id/textView18"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/fifteen"
                android:textColor="@color/text_black"
                android:textSize="@dimen/fifteen_thirteen"
                android:text="@string/Log_out" />


        </RelativeLayout>
    </LinearLayout>
    <RelativeLayout
        android:id="@+id/relativeLayout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true" >
    </RelativeLayout>
</RelativeLayout>

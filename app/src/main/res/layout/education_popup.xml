<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:id="@+id/relativeLayout"
    android:layout_height="match_parent">

    <RelativeLayout
        android:layout_width="@dimen/two_hundred_and_fifty"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_centerHorizontal="true"
        android:background="@drawable/kuang_corners_white">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/education_background"
            android:textSize="@dimen/eighteen_fifteen"
            android:id="@+id/textView24"
            android:textColor="@color/text_black"
            android:layout_marginTop="@dimen/fifteen"
            android:layout_marginBottom="@dimen/fifteen"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true" />

        <ImageView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/xian_gray"
            android:id="@+id/imageView21"
            android:layout_below="@+id/textView24"
            android:layout_centerHorizontal="true" />

        <RadioGroup
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/imageView21"
            android:layout_alignParentLeft="true"
            android:orientation="vertical"
            android:layout_alignParentStart="true">
            <RadioButton
                android:layout_width="match_parent"
                android:layout_height="@dimen/list_height"
                android:gravity="center|left"
                android:text="未填"
                android:drawableRight="@drawable/sex_selector"
                android:textColor="@color/select_red_white_color"
                android:textSize="@dimen/fifteen_thirteen"
                android:paddingRight="@dimen/fifteen"
                android:paddingLeft="@dimen/fifteen"
                android:button="@null"
                android:id="@+id/radioButton1" />

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/xian_gray"
                />
            <RadioButton
                android:layout_width="match_parent"
                android:layout_height="@dimen/list_height"
                android:gravity="center|left"
                android:text="高中及以下"
                android:drawableRight="@drawable/sex_selector"
                android:textColor="@color/select_red_white_color"
                android:textSize="@dimen/fifteen_thirteen"
                android:paddingRight="@dimen/fifteen"
                android:paddingLeft="@dimen/fifteen"
                android:button="@null"
                android:id="@+id/radioButton2" />
            <ImageView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/xian_gray"
                />
            <RadioButton
                android:layout_width="match_parent"
                android:layout_height="@dimen/list_height"
                android:gravity="center|left"
                android:text="大专"
                android:drawableRight="@drawable/sex_selector"
                android:textColor="@color/select_red_white_color"
                android:textSize="@dimen/fifteen_thirteen"
                android:paddingRight="@dimen/fifteen"
                android:paddingLeft="@dimen/fifteen"
                android:button="@null"
                android:id="@+id/radioButton3" />
            <ImageView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/xian_gray"
                />
            <RadioButton
                android:layout_width="match_parent"
                android:layout_height="@dimen/list_height"
                android:gravity="center|left"
                android:text="本科"
                android:drawableRight="@drawable/sex_selector"
                android:textColor="@color/select_red_white_color"
                android:textSize="@dimen/fifteen_thirteen"
                android:paddingRight="@dimen/fifteen"
                android:paddingLeft="@dimen/fifteen"
                android:button="@null"
                android:id="@+id/radioButton4" />
            <ImageView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/xian_gray"
                />
            <RadioButton
                android:layout_width="match_parent"
                android:layout_height="@dimen/list_height"
                android:gravity="center|left"
                android:text="硕士"
                android:drawableRight="@drawable/sex_selector"
                android:textColor="@color/select_red_white_color"
                android:textSize="@dimen/fifteen_thirteen"
                android:paddingRight="@dimen/fifteen"
                android:paddingLeft="@dimen/fifteen"
                android:button="@null"
                android:id="@+id/radioButton5" />
            <ImageView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/xian_gray"
                />
            <RadioButton
                android:layout_width="match_parent"
                android:layout_height="@dimen/list_height"
                android:gravity="center|left"
                android:text="博士"
                android:drawableRight="@drawable/sex_selector"
                android:textColor="@color/select_red_white_color"
                android:textSize="@dimen/fifteen_thirteen"
                android:paddingRight="@dimen/fifteen"
                android:paddingLeft="@dimen/fifteen"
                android:button="@null"
                android:id="@+id/radioButton6" />


        </RadioGroup>
    </RelativeLayout>
</RelativeLayout>
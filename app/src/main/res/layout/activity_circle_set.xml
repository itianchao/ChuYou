<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:clipToPadding="true"
    android:fitsSystemWindows="true"
    tools:context=".activity.circle.view.CircleSetActivity">
    <include
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        layout="@layout/ndian_title"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:id="@+id/include" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="@dimen/fifty"
        android:paddingRight="@dimen/fifteen"
        android:paddingLeft="@dimen/fifteen"
        android:text="@string/save"
        android:id="@+id/save"
        android:background="@null"
        android:gravity="center"
        android:textSize="@dimen/fifteen_thirteen"
        android:textColor="@color/press_white_gray_text"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />
    <EditText
        android:layout_width="match_parent"
        android:layout_height="@dimen/list_height"
        android:background="@drawable/kuang_input"
        android:layout_below="@+id/include"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:paddingLeft="@dimen/fifteen"
        android:paddingRight="@dimen/fifteen"
        android:textSize="@dimen/fifteen_thirteen"
        android:textColor="@color/text_black"
        android:gravity="center_vertical"
        android:singleLine="true"
        android:layout_marginTop="@dimen/fifteen"
        android:id="@+id/editText"></EditText>

</RelativeLayout>

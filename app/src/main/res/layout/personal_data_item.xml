<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/relativeLayout"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <LinearLayout
        android:id="@+id/linearLayout11"
        android:layout_width="fill_parent"
        android:layout_height="160dip"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:gravity="center_vertical"
        android:background="#f5f5f5"
        android:orientation="horizontal" >
        <wheel.widget.ChangeBirthLayout
            android:id="@+id/tv_birth"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#f5f5f5"
             />
    </LinearLayout>

    <RelativeLayout
        android:id="@+id/relativeLayout1"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_above="@+id/linearLayout11"
        android:layout_alignParentLeft="true"
        android:background="#f5f5f5" >

        <TextView
            android:id="@+id/Cancel"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/thirty_five"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/ten"
            android:gravity="center"
            android:text="@string/cancel"
            android:background="@drawable/kuang_corners_button_gray"
            android:layout_marginTop="@dimen/ten"
            android:layout_marginBottom="@dimen/ten"
            android:paddingLeft="@dimen/ten"
            android:paddingRight="@dimen/ten"
            android:textColor="@color/white"
            android:textSize="@dimen/fifteen_thirteen" />

        <TextView
            android:id="@+id/OK"
            android:background="@drawable/press_button_blue_white"
            android:textColor="@color/press_blue_white_color"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/thirty_five"
            android:layout_alignParentRight="true"
            android:paddingLeft="@dimen/ten"
            android:paddingRight="@dimen/ten"
            android:layout_centerVertical="true"
            android:layout_marginRight="@dimen/ten"
            android:gravity="center"
            android:text="@string/complete"
            android:textSize="@dimen/fifteen_thirteen" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="@string/birthday"
            android:id="@+id/textView20"
            android:layout_centerVertical="true"
            android:textSize="@dimen/eighteen_fifteen"
            android:textColor="@color/text_blue"
            android:layout_centerHorizontal="true" />
    </RelativeLayout>


    <ImageView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/imageView18"
        android:layout_above="@+id/linearLayout11"
        android:layout_alignParentLeft="true"
        android:background="@drawable/xian_gray"
        android:layout_alignParentStart="true" />

</RelativeLayout>